<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>

<jamoma version="0.4" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jamoma.org/ file:jamoma.xsd"
>
	
	<preset number="1" name="default">
		<item name="bitdepth"				type="msg_int">24</item>
		<item name="sr_ratio"				type="msg_float">1.</item>
		<item name="audio/gain/midi"		type="msg_int">0</item>
		<item name="audio/meters/freeze"	type="msg_int">0</item>
		<item name="audio/mix"				type="msg_float">0.</item>
		<item name="audio/bypass"			type="toggle">0</item>
		<item name="audio/mute"				type="toggle">0</item>
		<item name="audio/sample_rate"		type="msg_int">0</item>
    </preset>
	<preset number="2" name="crunchy">
		<item name="bitdepth" 				type="msg_int">5</item>
		<item name="sr_ratio"				type="msg_float">0.05</item>
		<item name="audio/gain/midi" 		type="msg_int">0</item>
		<item name="audio/meters/freeze" 	type="toggle">0</item>
		<item name="audio/mix" 				type="msg_float">0.</item>
		<item name="audio/bypass" 			type="toggle">0</item>
		<item name="audio/mute" 			type="toggle">0</item>
		<item name="audio/sample_rate" 		type="msg_int">0</item>
    </preset>
</jamoma>
	