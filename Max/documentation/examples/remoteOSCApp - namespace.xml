<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<jamoma version="0.6" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jamoma.org/ file:jamoma.xsd">
 <application author="jamoma team" version="v1" name="remoteOSCApp">
  <!--"an audio playback model"-->
  <player object="Container" tag="model" type="audio" priority="0">
   <!--"play the soundfile"-->
   <play object="Data" rangeBounds="0 1" priority="1" type="boolean" service="parameter"/>
   <!--"audio level"-->
   <gain object="Data" dataspace="gain" dataspaceUnit="dB" rampDrive="Max" rangeBounds="-100. 12." priority="0" type="decimal" valueStepsize="0.100000" valueDefault="0.000000" service="parameter" />
  </player>
  <!--"a filter model"-->
  <filter object="Container" tag="model" type="audio" priority="0">
   <!--"filter type"-->
   <type object="Data" priority="1" type="string" service="parameter"/>
   <!--"frequency of the filter"-->
   <frequency object="Data" dataspace="frequency" dataspaceUnit="Hz" rampDrive="Max" rangeBounds="0. 22050." rangeClipmode="low" priority="0" type="decimal" valueStepsize="0.100000" valueDefault="0.000000" service="parameter" />
  </filter>
 </application>
</jamoma>
